{
  "expo": {
    "name": "Zela Alerte",
    "slug": "zela-alerte",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": ["zela-alert"],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "entitlements": {
        "aps-environment": "development"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#3B82F6",
          "sounds": [
            "./assets/sounds/notification.wav"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.kaiser_styve.zela_alerte",
      "edgeToEdgeEnabled": true,
      "googleServicesFile": "./google-services.json"
    },
    "owner": "kaiser_styve",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "38b7d233-e0e1-4d15-aaff-59d39b20f60b"
      },
      "doctor": {
        "reactNativeDirectoryCheck": {
          "listUnknownPackages": false
        }
      }
    }
  }
}
